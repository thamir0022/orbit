{
  "extends": "@orbit/typescript-config/nest.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "incremental": false,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
    },
    "types": [
      "node"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "test",
    "dist",
    "**/*spec.ts"
  ]
}